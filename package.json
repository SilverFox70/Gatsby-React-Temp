  "scripts": {
    "build": "npm run build:client && npm run build:server",
    "build:client": "gatsby build --noUglify",
    "build:server": "NODE_ENV=production babel server -d public/server",
    "dev": "npm run develop",
    "dev:local": "concurrently \"npm run develop\" \"npm run dev:server\"",
    "dev:device": "concurrently \"npm run develop:device\" \"npm run dev:`server\"",
    "dev:server": "PORT=7000 babel-watch server/index.js",
    "develop": "gatsby develop",
    "develop:device": "gatsby develop -H 0.0.0.0",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "precommit": "pretty-quick --staged",
    "start": "npm run develop",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "gatsby"
  ],
